<!DOCTYPE html>
<html lang="en">
<head>
    <title>Microsoft Teams Hello World Sample App</title>
    @Styles.Render("~/Content/msteams-16.css")
    @Styles.Render("~/Content/Site.css")
    @Scripts.Render("https://statics.teams.microsoft.com/sdk/v1.0/js/MicrosoftTeams.min.js")
    @Scripts.Render("~/Scripts/teamsapp.js")

    @RenderSection("scripts", required: false)

    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script type="text/javascript">
      google.charts.load('current', {packages: ['corechart', 'gauge', 'calendar']});
      //google.charts.setOnLoadCallback(drawChart);
    </script>
</head>
<body class="theme-light">
    <div class="surface">
        <div class="panel">
            @RenderBody()
        </div>
    </div>
</body>
</html>
